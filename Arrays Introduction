#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
    int n;    
    cin>>n;
    int arr[n];

    for(int i=0;i<n;i++)
        cin>>arr[i];
    
    for(int j=n;j>=1;j--)
        cout<<arr[j-1]<<" ";
    
    return 0;
}
